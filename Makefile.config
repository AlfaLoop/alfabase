### JLINK Programmer
###
JLINK_DIR 		=
JLINK 			= $(JLINK_DIR)JLink.exe

#### Toolchain commands
####
GCC_INSTALL_ROOT	:=
GCC_VERSION			:=
GCC_SEASON			:=
GCC_PREFIX			:= arm-none-eabi

CC      := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-gcc
CXX     := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-g++
AS      := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-gcc
AR      := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-ar
LD      := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-gcc
NM      := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-nm
OBJDUMP := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-objdump
OBJCOPY := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-objcopy
GDB     := $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-gdb
SIZE	:= $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-size
STRIP	:= $(GCC_INSTALL_ROOT)/$(GCC_VERSION)/$(GCC_SEASON)/bin/$(GCC_PREFIX)-strip
